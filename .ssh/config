Host vm*
     User root
     IdentityFile ~/.ssh/id_autoit
     StrictHostKeyChecking no
     UserKnownHostsFile /dev/null
     LogLevel ERROR

Host lgnas
     HostName 192.168.1.101
     User lgfang
     # NOTE: do NOT set "ControlMaster Auto" for "host *"
     ControlMaster auto

Host *
     User lgfang
     Protocol 2,1
     ServerAliveInterval 180
     ForwardX11 no
     # Compression yes
     # ControlPath /tmp/%r_ssh_%h_%p
